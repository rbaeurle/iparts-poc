<Message xmlns="http://asplm.daimler.com/MediaService/v1"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://asplm.daimler.com/MediaService/v1 asplm_message_flat.xsd">
    <Response from="ASPLM" to="iParts" requestId="r0001" requestOperation="CreateMediaOrder">
        <Success ok="false" errorcode="01">
            <Errortext textId="error003" lang="de">Inkorrekter Request (Invalides XML)</Errortext>
            <Errortext textId="error003" lang="en">Incorrect Request (invalid XML)</Errortext>
        </Success>
    </Response>
    <History>
        <![CDATA[<Request from="iParts" to="ASPLM" requestId="req_e01">
            <Requestor userId="markovic"/>
            <Operation>
                <CreateMediaOrder>
                    <Name>Proposed Name</Name>
                    <Realization>Strichzeichnung</Realization>
                    <DateDue>2015-06-30</DateDue>
                    <EinPAS mainGroup="20" group="16" technicalScope="00">Luftfederung</EinPAS>
                    <Usage><ModelSeries>204</ModelSeries></Usage>
                </CreateMediaOrder>
            </Operation>
        </Request>]]>
    </History>
</Message>