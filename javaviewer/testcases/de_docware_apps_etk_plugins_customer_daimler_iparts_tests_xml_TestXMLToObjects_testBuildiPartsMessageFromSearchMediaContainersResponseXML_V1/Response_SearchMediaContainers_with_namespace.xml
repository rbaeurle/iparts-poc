<a:Message xmlns:a="http://asplm.daimler.com/MediaService/v1">
    <a:Response from="ASPLM" requestId="req_s01" to="iParts" requestOperation="SearchMediaContainers">
        <a:Success ok="true"/>
        <a:ResSearchMediaContainers numResultsDelivered="2">
            <a:MediaContainer mcItemId="MC000.010.000.023" mcItemRevId="001">
                <a:Attr name="name">B2156</a:Attr>               
                <a:Attr name="description">xyz</a:Attr>               
            </a:MediaContainer>
            <a:MediaContainer mcItemId="MC000.010.000.087" mcItemRevId="001">
                <a:Attr name="name">B6732</a:Attr>               
                <a:Attr name="description">abc</a:Attr>               
            </a:MediaContainer>
        </a:ResSearchMediaContainers>
    </a:Response>
    <a:History>
        <a:Request from="iParts" requestId="req_s01" to="ASPLM">
            <a:Requestor userId="migimport"/>
            <a:Operation>
                <a:SearchMediaContainers maxResults="300">
                    <a:SearchCriterion attrName="name">B*</a:SearchCriterion>
                    <a:ResultAttribute attrName="name"/>
                    <a:ResultAttribute attrName="description"/>
                </a:SearchMediaContainers>
            </a:Operation>
        </a:Request>
    </a:History>
</a:Message>