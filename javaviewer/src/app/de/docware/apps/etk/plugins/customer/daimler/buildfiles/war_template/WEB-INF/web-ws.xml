<web-app xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" metadata-complete="true" version="2.4" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">

  <context-param>
    <param-name>mainClass</param-name>
    <param-value>de.docware.apps.etk.plugins.customer.daimler.iparts.application.iPartsFrameworkMain</param-value>
  </context-param>

  <listener>
    <listener-class>de.docware.framework.modules.gui.output.j2ee.J2EEServlet</listener-class>
  </listener>

  <servlet>
    <servlet-name>J2EEServlet</servlet-name>
    <servlet-class>de.docware.framework.modules.gui.output.j2ee.J2EEServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>

  <servlet-mapping>
    <servlet-name>J2EEServlet</servlet-name>
    <url-pattern>/*</url-pattern>
  </servlet-mapping>

  <session-config>
    <session-timeout>10</session-timeout>
  </session-config>

</web-app>

