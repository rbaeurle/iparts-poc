<?xml version="1.0" encoding="UTF-8"?><xs:schema xml:lang="en" xmlns:xs="http://www.w3.org/2001/XMLSchema"  xmlns:jxb="http://java.sun.com/xml/ns/jaxb" jxb:version="1.0">  <!-- This file is controlled by DCCS SVN and must not be changed otherwise.       Usage of attributes preferred over use of tags.       Created by DCCS GmbH. on behalf of Daimler AG.       Copyright 2012 Daimler AG and DCCS GmbH.       Author:       Peter.Stuecklschwaiger@DCCS.at       Responsible:  Tomislav.Jerkovic@Daimler.com, Swen.Koelbel@Daimler.com -->	<xs:annotation>		<xs:documentation>Copyright 2012 Daimler AG and DCCS GmbH      based on GLOBUSCommonDefinitions (Copyright 2007 Daimler AG and IBM Corporation)</xs:documentation>	</xs:annotation>  <xs:simpleType name="InterfaceType">    <xs:annotation><xs:documentation>Defines unique value for different interface types.        Used to distinguish messages in an easy manner. Details on messages are available        with the specific message types.</xs:documentation></xs:annotation>    <xs:restriction base="xs:token">      <xs:enumeration value="ANY">        <xs:annotation><xs:documentation>Messages not related to any specific PACE application.        </xs:documentation></xs:annotation></xs:enumeration>      <xs:enumeration value="CTIME">        <xs:annotation><xs:documentation>Messages related to ctime.</xs:documentation>        </xs:annotation></xs:enumeration>      <xs:enumeration value="EINSTEIN">        <xs:annotation><xs:documentation>Messages related to Einstein.</xs:documentation>        </xs:annotation></xs:enumeration>      <xs:enumeration value="FERRET">        <xs:annotation><xs:documentation>Messages related to FERRET.</xs:documentation>        </xs:annotation></xs:enumeration>      <xs:enumeration value="KODABA">        <xs:annotation><xs:documentation>Messages related to KoDaBa.</xs:documentation>        </xs:annotation></xs:enumeration>      <xs:enumeration value="SAFE">        <xs:annotation><xs:documentation>Messages related to SaFE.</xs:documentation>        </xs:annotation></xs:enumeration>    </xs:restriction>  </xs:simpleType>  <xs:simpleType name="SystemType">    <xs:annotation><xs:documentation>Defines unique value for different systems.        Used to distinguish messages and partners in an easy manner.        IMPORTANT: only registered systems can be used!</xs:documentation></xs:annotation>    <xs:restriction base="xs:string">      <xs:enumeration value="AMSupply">        <xs:annotation><xs:documentation>Automotive Supply - e.g. start</xs:documentation>        </xs:annotation></xs:enumeration>      <xs:enumeration value="eDocs">        <xs:annotation><xs:documentation>The documentation system.</xs:documentation>        </xs:annotation></xs:enumeration>      <xs:enumeration value="GSP">        <xs:annotation><xs:documentation>PACE - the mother of ctime, SaFE, Einstein and KoDaBa.        </xs:documentation></xs:annotation></xs:enumeration>      <xs:enumeration value="PACE">        <xs:annotation><xs:documentation>PACE - the mother of ctime, SaFE, Einstein and KoDaBa.        </xs:documentation></xs:annotation></xs:enumeration>      <xs:enumeration value="Primus">        <xs:annotation><xs:documentation>The supplier document system.</xs:documentation>        </xs:annotation></xs:enumeration>      <xs:enumeration value="SQMS">        <xs:annotation><xs:documentation>A quality system.</xs:documentation>        </xs:annotation></xs:enumeration>    </xs:restriction>  </xs:simpleType>  <xs:simpleType name="VersionType">    <xs:annotation><xs:documentation>Defines a common format to use for version numbers (nn-nn-nn).    </xs:documentation></xs:annotation>    <xs:restriction base="xs:string">      <xs:pattern value="[0-9]{1,2}\.[0-9]{1,2}\.[0-9]{1,2}"/>    </xs:restriction>  </xs:simpleType>  <xs:simpleType name="EnvironmentType">    <xs:annotation><xs:documentation>Defines the runtime environments of the interface.        e.g. develop, test, maintenance, production    </xs:documentation></xs:annotation>    <xs:restriction base="xs:token">      <xs:enumeration value="DEVELOP"/>      <xs:enumeration value="UAT">        <xs:annotation><xs:documentation>The environment for User Acceptance Tests.</xs:documentation>        </xs:annotation></xs:enumeration>      <xs:enumeration value="MAINTENANCE"/>      <xs:enumeration value="PRODUCTION"/>    </xs:restriction>  </xs:simpleType>  <xs:attributeGroup name="interior">    <xs:annotation><xs:documentation>The attributes needed for the description of the        interior (that is: PACE) system.</xs:documentation></xs:annotation>    <xs:attribute name="interiorSystem" type="SystemType" fixed="PACE"/>    <xs:attribute name="interiorInterface" type="InterfaceType" use="required"/>    <xs:attribute name="interiorEnvironment" type="EnvironmentType" use="required"/>    <xs:attribute name="interiorVersion" type="VersionType" use="required"/>  </xs:attributeGroup>  <xs:attributeGroup name="foreign">    <xs:annotation><xs:documentation>The attributes needed for the description of the        foreign system.</xs:documentation></xs:annotation>    <xs:attribute name="foreignSystem" type="SystemType" use="required"/>    <xs:attribute name="foreignEnvironment" type="EnvironmentType" use="required"/>    <xs:attribute name="foreignVersion" type="VersionType" use="required"/>  </xs:attributeGroup></xs:schema>