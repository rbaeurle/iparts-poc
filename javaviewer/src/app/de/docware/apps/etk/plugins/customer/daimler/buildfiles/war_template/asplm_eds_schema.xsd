    <xs:element name="S05Remarks">
        <xs:annotation>
            <xs:documentation>Remarks can be assigned to positions in (modular) PartsLists (=S05-structures).</xs:documentation>
            <xs:documentation>Maximal ten different remarks to positions of PartsList can be defined.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="S05Remark" maxOccurs="10" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Every remark to an S05 is assigned using digits from 0 to 9 (stored in attribute Nr).</xs:documentation>
                        <xs:documentation>BOM-DB: Column [suffix]_BEMZ_TEXT[0-9]</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:simpleContent>
                            <xs:extension base="Typ_S05Remark">
                                <xs:attribute name="nr">
                                    <xs:simpleType>
                                        <xs:restriction base="xs:string"> </xs:restriction>
                                    </xs:simpleType>
                                </xs:attribute>
                            </xs:extension>
                        </xs:simpleContent>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
