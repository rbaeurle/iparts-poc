            <xs:sequence>
                <xs:element maxOccurs="unbounded" minOccurs="0" name="InvolvedDesignRelease"
                    type="Typ_DesignRelease">
                    <xs:annotation>
                        <xs:documentation>Key for the department/division which is involved.</xs:documentation>
                        <xs:documentation>BOM-DB: Extract of suffix_BETKF</xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
