        </xs:annotation>
    </xs:element>
    <xs:element name="EngineeringDateFrom" type="Typ_BomDbDate">
        <xs:annotation>
            <xs:documentation>Date/time from which on the parent element is valid (engineering view)</xs:documentation>
            <xs:documentation>BOM-DB: Column [suffix]_S_DAT_AB</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="EngineeringDateTo" type="Typ_BomDbDate">
        <xs:annotation>
            <xs:documentation>Date/time up to which the parent element is valid (engineering view)</xs:documentation>
            <xs:documentation>BOM-DB: Column [suffix]_S_DAT_BIS</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="ManualFlagFrom">
        <xs:annotation>
            <xs:documentation>engineeringDateFrom of parent element is manually shifted if content is 'M'</xs:documentation>
            <xs:documentation>BOM-DB: Column [suffix]_MKZ_AB</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
            <xs:restriction base="xs:string">
                <xs:maxLength value="1"/>
            </xs:restriction>
        </xs:simpleType>
    </xs:element>
    <xs:element name="ManualFlagTo">
        <xs:annotation>
            <xs:documentation>engineeringDateTo of parent element is manually shifted if content is 'M'</xs:documentation>
            <xs:documentation>BOM-DB: Column [suffix]_MKZ_BIS</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
            <xs:restriction base="xs:string">
                <xs:maxLength value="1"/>
            </xs:restriction>
        </xs:simpleType>
    </xs:element>
    <xs:element name="Eco">
        <xs:annotation>
            <xs:documentation>Engineering Change Object (ECO) / KonstruktionsEinsatzMeldung (KEM)</xs:documentation>
            <xs:documentation>BOM-DB: Column [suffix]_SNR for table KEM and KEMD</xs:documentation>
            <xs:documentation>BOM-DB: Column [suffix]_KEM for table K3 and AEH</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
            <xs:restriction base="xs:string">
                <xs:maxLength value="13"/>
            </xs:restriction>
        </xs:simpleType>
    </xs:element>
    <xs:element name="EcoFrom">
        <xs:annotation>
            <xs:documentation>Engineering Change Order creating the versionFrom of object/structure</xs:documentation>
            <xs:documentation>BOM-DB: Column [suffix]_KEM_AB</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
            <xs:restriction base="xs:string">
                <xs:maxLength value="13"/>
            </xs:restriction>
        </xs:simpleType>
    </xs:element>
    <xs:element name="EcoTo">
        <xs:annotation>
            <xs:documentation>Engineering Change Order closing the versionTo of object/structure</xs:documentation>
            <xs:documentation>BOM-DB: Column [suffix]_KEM_BIS</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
            <xs:restriction base="xs:string">
                <xs:maxLength value="13"/>
            </xs:restriction>
        </xs:simpleType>
    </xs:element>
    <xs:element name="VersionFrom" type="Typ_Version">
        <xs:annotation>
            <xs:documentation>Version of object/structure, created by EcoFrom</xs:documentation>
            <xs:documentation>BOM-DB: Column [suffix]_AS_AB</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="VersionTo" type="Typ_Version">
        <xs:annotation>
            <xs:documentation>Version of object/structure, closed by EcoFrom</xs:documentation>
            <xs:documentation>BOM-DB: Column [suffix]_AS_BIS</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="VersionToInvalidFlag">
        <xs:annotation>
            <xs:documentation>States if versionTo of object/structure is invalidated</xs:documentation>
            <xs:documentation>Flag set to ‘U’ means versionTo of Object/structure is not valid (possible values are U and space).</xs:documentation>
            <xs:documentation>BOM-DB: Column [suffix]_UNG_KZ_BIS</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
            <xs:restriction base="xs:string">
                <xs:maxLength value="1"/>
            </xs:restriction>
        </xs:simpleType>
    </xs:element>
    <xs:element name="StatusFrom">
        <xs:annotation>
            <xs:documentation>Status of version of object/structure, version generated by ecoFrom</xs:documentation>
            <xs:documentation>BOM-DB, Column [suffix]_VAKZ_AB</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
            <xs:restriction base="xs:string">
                <xs:maxLength value="2"/>
            </xs:restriction>
        </xs:simpleType>
    </xs:element>
    <xs:element name="StatusTo">
        <xs:annotation>
            <xs:documentation>Status of version of object/structure, version generated by ecoTo</xs:documentation>
            <xs:documentation>BOM-DB, Column [suffix]_VAKZ_BIS</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
            <xs:restriction base="xs:string">
                <xs:maxLength value="2"/>
            </xs:restriction>
        </xs:simpleType>
    </xs:element>
    <xs:element name="SteeringType">
        <xs:annotation>
            <xs:documentation>BOM-DB: Column B2I_LKG/BK_LKG/COD_LKG_1/COD_LKG_2</xs:documentation>
            <xs:documentation>Values 0 and 4 are mapped to K,
values 1 and 5 are mapped to L (left), 
values 2 and 6 are mapped to R (right),
otherwise the value is Space.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
            <xs:restriction base="xs:string">
                <xs:maxLength value="1"/>
            </xs:restriction>
        </xs:simpleType>
    </xs:element>
    <xs:element name="MaturityLevel">
        <xs:simpleType>
            <xs:restriction base="xs:string">
                <xs:maxLength value="1"/>
            </xs:restriction>
        </xs:simpleType>
    </xs:element>
