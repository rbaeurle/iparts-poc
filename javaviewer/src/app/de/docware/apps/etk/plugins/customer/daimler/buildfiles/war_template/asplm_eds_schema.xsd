                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="43"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element minOccurs="0" name="AdditionalResponsible1">
                                <xs:annotation>
                                    <xs:documentation>BOM-DB: First KEM_AUFREG_EXT_xy which can’t be mapped and for which digit 21 to 43 is not completely empty</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="43"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element minOccurs="0" name="AdditionalResponsible2">
                                <xs:annotation>
                                    <xs:documentation>BOM-DB: Second KEM_AUFREG_EXT_xy which can’t be mapped and for which digit 21 to 43 is not completely empty</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="43"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:group minOccurs="0" ref="Group_DesignReleases" maxOccurs="1"/>
                <xs:element minOccurs="0" ref="PlantSuppliesEds"/>
                <xs:element minOccurs="0" ref="PlantSuppliesBcs"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
