                        <xs:sequence>
                            <xs:element name="AdditionalMaterialQuality" maxOccurs="36">
                                <xs:annotation>
                                    <xs:documentation>Shows additional DBL</xs:documentation>
                                    <xs:documentation>BOM-DB: Column TEIE_DBN</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="18"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
