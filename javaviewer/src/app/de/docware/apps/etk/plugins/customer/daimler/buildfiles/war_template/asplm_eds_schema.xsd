<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
    <xs:element name="Dataset">
        <xs:annotation>
            <xs:documentation>Enthält BOM-DB-Datensätze "sortenrein", d.h. alle Unterelemente stammen aus der gleichen Tabelle (deren Name im Attribut name angegeben wird).</xs:documentation>
            <xs:documentation>Dies ist die Struktur die Daten, die an die Abnehmersysteme (iParts, ASPLM usw.) geliefert wird.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:choice>
                <xs:sequence>
                    <xs:element maxOccurs="unbounded" ref="B90"/>
                </xs:sequence>
                <xs:sequence>
                    <xs:element maxOccurs="unbounded" ref="S05"/>
                </xs:sequence>
                <xs:sequence>
                    <xs:element maxOccurs="unbounded" ref="PartMasterData"/>
                </xs:sequence>
                <xs:sequence>
                    <xs:element maxOccurs="unbounded" ref="PartMasterData_LDI"/>
                </xs:sequence>
                <xs:sequence>
                    <xs:element maxOccurs="unbounded" ref="PartSpecialMasterData"/>
                </xs:sequence>
                <xs:sequence>
                    <xs:element maxOccurs="unbounded" ref="PartAdditionalMasterData"/>
                </xs:sequence>
                <xs:sequence>
                    <xs:element maxOccurs="unbounded" ref="CodeMasterData"/>
                </xs:sequence>
                <xs:sequence>
                    <xs:element maxOccurs="unbounded" ref="CodeMasterData_LDI"/>
                </xs:sequence>
                <xs:sequence>
                    <xs:element maxOccurs="unbounded" ref="ScopeMasterData"/>
                </xs:sequence>
                <xs:sequence>
                    <xs:element maxOccurs="unbounded" ref="PartsListMasterData"/>
                </xs:sequence>
                <xs:sequence>
                    <xs:element maxOccurs="unbounded" ref="PartsListAdditionalMasterData"/>
                </xs:sequence>
                <xs:sequence>
                    <xs:element maxOccurs="unbounded" ref="PartsListMasterData_LDI"/>
                </xs:sequence>
                <xs:sequence>
                    <xs:element maxOccurs="unbounded" ref="S05PointOfUsageText"/>
                </xs:sequence>
                <xs:sequence>
                    <xs:element maxOccurs="unbounded" ref="ModelMasterData"/>
                </xs:sequence>
                <xs:sequence>
                    <xs:element maxOccurs="unbounded" ref="ModelMasterData_LDI"/>
                </xs:sequence>
                <xs:sequence>
                    <xs:element maxOccurs="unbounded" ref="ModuleMasterData"/>
                </xs:sequence>
                <xs:sequence>
                    <xs:element maxOccurs="unbounded" ref="ModuleMasterData_LDI"/>
                </xs:sequence>
                <xs:sequence>
                    <xs:element maxOccurs="unbounded" ref="EcoMasterData"/>
                </xs:sequence>
                <xs:sequence>
                    <xs:element maxOccurs="unbounded" ref="EcoMasterData_LDI"/>
                </xs:sequence>
                <xs:sequence>
                    <xs:element maxOccurs="unbounded" ref="K3"/>
                </xs:sequence>
                <xs:sequence>
                    <xs:element maxOccurs="unbounded" ref="ChangeNotice"/>
                </xs:sequence>
            </xs:choice>
            <xs:attribute name="name" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Der Name der BOM-DB-Tabelle, aus der die Datensätze stammen.</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute ref="origin" use="optional"/>
            <xs:attribute ref="sourceExportTime"/>
            <xs:attribute ref="trafoTime"/>
            <xs:attribute ref="schemaVersion"/>
            <xs:attribute ref="versionSVN"/>
            <xs:attribute ref="changedDate"/>
        </xs:complexType>
    </xs:element>
    <xs:attribute name="origin" type="xs:string">
        <xs:annotation>
            <xs:documentation>Gibt den Ursprung der Daten, also das erzeugende System an.</xs:documentation>
            <xs:documentation>Wenn dieses Attribut im Element Dataset gesetzt ist, vererbt es sich automatisch auf alle darin enthaltenen Datensätze mit Ausnahme derer, die ein eigenes Attribut origin besitzen.</xs:documentation>
        </xs:annotation>
    </xs:attribute>
    <xs:attribute name="sourceExportTime" type="xs:dateTime">
        <xs:annotation>
            <xs:documentation>Zeitpunkt des Datenexports im erzeugenden System</xs:documentation>
        </xs:annotation>
    </xs:attribute>
    <xs:attribute name="trafoTime" type="xs:dateTime">
        <xs:annotation>
            <xs:documentation>Zeitpunkt der Level1-Transformation</xs:documentation>
        </xs:annotation>
    </xs:attribute>
    <xs:attribute fixed="1.0" name="schemaVersion" type="xs:NMTOKEN">
        <xs:annotation>
            <xs:documentation>Version des aktuellen Schemas.</xs:documentation>
        </xs:annotation>
    </xs:attribute>
    <xs:attribute name="versionSVN" type="xs:string">
        <xs:annotation>
            <xs:documentation>SVN version des aktuellen Schemas</xs:documentation>
        </xs:annotation>
    </xs:attribute>
    <xs:attribute fixed="$LastChangedRevision: 11638 $" name="revision" type="xs:string">
        <xs:annotation>
            <xs:documentation>SVN version des aktuellen Schemas</xs:documentation>
        </xs:annotation>
    </xs:attribute>
    <xs:attribute name="changedDate" type="xs:string">
        <xs:annotation>
            <xs:documentation>Datum der letzten Änderung in Subversion</xs:documentation>
        </xs:annotation>
    </xs:attribute>
    <xs:attribute fixed="$LastChangedDate: 2017-04-27 17:31:59 +0200 (Do, 27 Apr 2017) $"
        name="lastChangedDate" type="xs:string">
        <xs:annotation>
            <xs:documentation>Datum der letzten Änderung in Subversion</xs:documentation>
        </xs:annotation>
    </xs:attribute>
    <xs:element name="B90">
        <xs:annotation>
            <xs:documentation>getT43RB2I: Service operation provides the usage data of one or more given items in one or more Baumuster (=Fahrzeugbaumuster or Aggregat) and reverse the Service provides the content of one or more given Baumuster.</xs:documentation>
            <xs:documentation>The service provides the usage data of one or more given Items in one or more Models (=Vehicles or MajorComponents) and reverse the Service provides the content (=Parts or Assemblies or Parts Lists or XK-Items or MajorComponents) of one or more given Models.</xs:documentation>
            <xs:documentation>The usage relation resp. content relation is called B90-structure, abbreviated B90.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Model" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Vehicle or MajorComponent</xs:documentation>
                        <xs:documentation>BOM-DB: B2I-SNR, characters 1 to 13 (Typo?)</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="7"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="Module" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Module of Model</xs:documentation>
                        <xs:documentation>BOM-DB: B2I-SNR, characters 14-17</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="4"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="Scope" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Scope within Model/Module</xs:documentation>
                        <xs:documentation>BOM-DB: B2I-UMF</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="7"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="Position" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Position within Model / Module / Scope</xs:documentation>
                        <xs:documentation>BOM-DB: B2I-POS</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:integer">
                            <xs:minInclusive value="0"/>
                            <xs:maxInclusive value="999"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element minOccurs="0" ref="Item">
                    <xs:annotation>
                        <xs:documentation>Where-used object</xs:documentation>
                        <xs:documentation>BOM-DB: B2I-SNRU</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:group minOccurs="0" ref="Group_ReleaseDates"/>
                <xs:group minOccurs="0" ref="Group_EngineeringDates"/>
                <xs:group minOccurs="0" ref="Group_ManualFlags"/>
                <xs:group minOccurs="0" ref="Group_Eco"/>
                <xs:group minOccurs="0" ref="Group_Versions"/>
                <xs:group minOccurs="0" ref="Group_Status"/>
                <xs:element minOccurs="0" ref="SteeringType">
                    <xs:annotation>
                        <xs:documentation>Limitation of steering of the B90 (optional - if this element is missing, there is no limitation in steering)</xs:documentation>
                        <xs:documentation>BOM-DB: B2I_LKG. Value 1 and 5 are mapped to L (left), values 2 and 6 are mapped to R (right). Third possible value is Space.</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="BodyTypes" minOccurs="0">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element maxOccurs="unbounded" minOccurs="0" name="BodyType">
                                <xs:annotation>
                                    <xs:documentation>Limitation of types of body of B90</xs:documentation>
                                    <xs:documentation>Up to four body types could be assigned to a Model. Possible values are 1, 2, 3, 9.</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="1"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" ref="MaturityLevel">
                    <xs:annotation>
                        <xs:documentation>Level of maturity of B90 (S means stopped)</xs:documentation>
                        <xs:documentation>BOM-DB: B2I_RF</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="Quantity" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Quantity of where-used object</xs:documentation>
                        <xs:documentation>BOM-DB: B21_MG (2 characters)</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="2"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element minOccurs="0" name="ProductGroup">
                    <xs:annotation>
                        <xs:documentation>BOM-DB: B21_PGKZ (N=Trucks, S=Buses)</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="1"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element minOccurs="0" name="Type">
                    <xs:annotation>
                        <xs:documentation>Type of B90</xs:documentation>
                        <xs:documentation>BOM-DB: B2I_BGKZ</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="2"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element minOccurs="0" name="CodeRule">
                    <xs:annotation>
                        <xs:documentation>Code rule of B90</xs:documentation>
                        <xs:documentation>BOM-DB: B2I_CDBED</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="150"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element minOccurs="0" ref="PlantSupplies"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="S05">
        <xs:annotation>
            <xs:documentation>getT43RBK: This element contains the usage data of one or more given Items (Teile or Zusammenbauten = ZB; Parts or Assemblies) in one or more ModularPartsLists (Zusammenbauten = ZB or Stücklistenausführungsarten = SAA; Assemblies or PartsLists), and reverse the Service provides the content (Teile or ZB) of one or more given ModularPartsLists (ZB or SAA).</xs:documentation>
            <xs:documentation>ModularPartsLists and PartsLists are used synonymous.</xs:documentation>
            <xs:documentation>The usage relation resp. content relation is called S05-structure, abbreviated S05.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="PartsList" minOccurs="0" type="Typ_PartsList13">
                    <xs:annotation>
                        <xs:documentation>PartsList or Assembly</xs:documentation>
                        <xs:documentation>BOM-DB: BK-SNR</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="Position" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Position within ModularPartsList</xs:documentation>
                        <xs:documentation>BOM-DB: BK-POS (9 digits, but the first three are always 000 so they can be ignored)</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:integer">
                            <xs:minInclusive value="0"/>
                            <xs:maxInclusive value="999999"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element ref="Item" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Part or Assembly</xs:documentation>
                        <xs:documentation>BOM-DB: BK-SNRU</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:group minOccurs="0" ref="Group_ReleaseDates"/>
                <xs:group minOccurs="0" ref="Group_Eco"/>
                <xs:group minOccurs="0" ref="Group_Versions"/>
                <xs:group minOccurs="0" ref="Group_Status"/>
                <xs:element name="RemarkDigit" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Remarks to S05-structure are assigned using a digit from 0 to 9 or ‘+’</xs:documentation>
                        <xs:documentation>BOM-DB: BK-BEMZ</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="1"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="AlternativeFlag" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>In case of Parts or Assemblies may be used alternatively, in S05-structure a character is assigned.</xs:documentation>
                        <xs:documentation>A or B or … or Z or Space [remark: value ‘*’ is a mistake in EDS/BCS, might be ignored].</xs:documentation>
                        <xs:documentation>BOM-DB: BK-WWKB</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="1"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="Quantity" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Quantity of where-used-object</xs:documentation>
                        <xs:documentation>Maximal 9999 with three decimal places or ‘N.BED’ resp. ‘+’</xs:documentation>
                        <xs:documentation>BK_MENGE including service transformations by checking BKMGKZ:</xs:documentation>
                        <xs:documentation>Service transformation:
If BK-MGKZ Space: quantiy := BK_MENGE
If BK-MGKZ = ‘B’: quantity := ‘N.BED’
If BK-MGKZ = ‘+’: quantity := ‘+’</xs:documentation>
                        <xs:documentation>TODO: Klären, wieviele Stellen - 4 + 3 + 1 = 8</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="8"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element minOccurs="0" ref="SteeringType">
                    <xs:annotation>
                        <xs:documentation>Limitation of steering of S05</xs:documentation>
                        <xs:documentation>Contents: L or R or Space (see also the comments for the global element definition!)</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="DisqualifyFlag" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Used Parts or Assemblies in S05 could be disqualified by
assigning a ‘-‘ (=Minus)</xs:documentation>
                        <xs:documentation>Applied only in the old and idle EDS/BCS Documentation</xs:documentation>
                        <xs:documentation>BOM-DB: BK-PLMI</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="1"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="VariantsFlag" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Applied in EDS/BCSDocumentation done by EvoBus</xs:documentation>
                        <xs:documentation>BOM-DB: BK-VARIANTEN-KZ</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="1"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="VariableConstructionMeasure" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Applied in EDS/BCS Documentation done by EvoBus</xs:documentation>
                        <xs:documentation>BOM-DB: BK-VARKONSTR-MASS</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="10"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element ref="MaturityLevel" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Level of maturity of parent element (S means stopped)</xs:documentation>
                        <xs:documentation>BOM-DB: BK_RF</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element ref="PlantSupplies" minOccurs="0"/>
                <xs:element name="AcquisitionType" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Defines who produces or procures the item and for whom it is produced or procured.</xs:documentation>
                        <xs:documentation>BOM-DB: BK-BZA</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="12"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="PipePartsListFlag" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Flag to identify Pipes, which are documented as ModularPartsLists</xs:documentation>
                        <xs:documentation>BOM-DB: BK-LTG-BK</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="1"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="PartMasterData">
        <xs:annotation>
            <xs:documentation>getT43RTEIL</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="PartNumber" minOccurs="0"/>
                <xs:element minOccurs="0" ref="PartLangData">
                    <xs:annotation>
                        <xs:documentation>This is only the German version of the language dependent data (stored in the main table T43RTEIL).</xs:documentation>
                        <xs:documentation>The corresponding information in other languages comes from table T43RTEIS and is stored in a separate element PartMasterData_LDI.</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:group minOccurs="0" ref="Group_ReleaseDates"/>
                <xs:group minOccurs="0" ref="Group_Eco"/>
                <xs:group minOccurs="0" ref="Group_Versions"/>
                <xs:group minOccurs="0" ref="Group_Status"/>
                <xs:element minOccurs="0" ref="DesignChangeCounter"/>
                <xs:element name="FlashwareType" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Type of Flashware part ('HW'=Hardware, 'FW'=Flashware, 'D'=Diagnostics, 'S'=Software, ''=unset/unknown)</xs:documentation>
                        <xs:documentation>BOM-DB: Column TEIL_FW_KZ</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="2"/>
                            <!-- Auf Anforderung des FB entfernt
	                            <xs:enumeration value="HW"/>
	                            <xs:enumeration value="FW"/>
	                            <xs:enumeration value="D"/>
	                            <xs:enumeration value="S"/>
	                            <xs:enumeration value=""/>
                             -->
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="PartsType" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Indicates additional abbreviation, if the part is assembly, installation drawing or similar.</xs:documentation>
                        <xs:documentation>BOM-DB: Column TEIL_ZBKZ</xs:documentation>
                        <xs:documentation>
                        	Possible values:
							ZB (assembly), 
							EB (installation drawing), 
							AO (arrangement drawing),
							LU (supplier part),
							TB (table drawing) etc.
						</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="2"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="AssemblyStructureType" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>BOM-DB: Column TEIL_ZBF</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="1"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="DrawingGeometryVersion" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Shows the drawing version in Smaragd.</xs:documentation>
                        <xs:documentation>The leading zero from BOM-DB is ignored (0123 becomes 123)</xs:documentation>
                        <xs:documentation>BOM-DB: Column TEIL_ZGS</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <!-- Auf Anforderung des FB entfernt
                            <xs:pattern value="[0-9]*"/>
                            -->
                            <xs:maxLength value="4"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="DrawingDateOrType" minOccurs="0">
                    <!-- Auf Anforderung des FB aufgeweicht Attribut "flag" entfernt. Auslieferung nur ueber den Knotenwert-->
                    <xs:annotation>
                        <xs:documentation>Shows if the part is drawn or not.</xs:documentation>
                        <xs:documentation>If a drawing is available, its date is given as content. Otherwise the content describes the reason why there is no drawing (values UV, GEO etc.).</xs:documentation>
                        <xs:documentation>BOM-DB: Column TEIL_ZDAT</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="6"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="ReferencePart" type="Typ_PartNumber" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Shows reference part which indicates the part that stores further drawing information.</xs:documentation>
                        <xs:documentation>BOM-DB: Column TEIL_ZSN</xs:documentation>
                        <xs:documentation>Attribute in EDS: BEZUGS-SNR or Bezugszeichnung</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="ReferenceDrawing" type="Typ_PartNumber" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Shows reference drawing which the part is drawn on, because it has no own drawing.</xs:documentation>
                        <xs:documentation>BOB-DB: Column TEIL_HZSN</xs:documentation>
                        <xs:documentation>Attribute in EDS: SIEHESNR or Hinweiszeichnung</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="PartReleaseStatus" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>BOM-DB: Column TEIL_FGST</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="2"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="QuantityUnit" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Shows quantity unit of the part relating to its whole usage.</xs:documentation>
                        <xs:documentation>BOM-DB: Column TEIL_ME</xs:documentation>
                        <xs:documentation>
                        	Possible values:
							01 (piece), 
							02 (kg), 
							03 (meter) etc.
						</xs:documentation>
