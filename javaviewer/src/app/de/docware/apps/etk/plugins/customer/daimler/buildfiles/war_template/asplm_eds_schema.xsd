                        <xs:documentation>Item, to which  change notices are defined .</xs:documentation>
                        <xs:documentation>BOM-DB: Column AEH-SNR</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="17"/>
