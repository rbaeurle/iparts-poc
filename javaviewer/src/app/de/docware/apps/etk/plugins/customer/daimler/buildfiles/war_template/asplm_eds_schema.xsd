                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="1"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element minOccurs="0" name="FinishedPart">
                                <xs:annotation>
                                    <xs:documentation>Only in EDS: RFME-Fertigteil.</xs:documentation>
                                    <xs:documentation>BOM-DB: Column AEH-RFME-F</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="1"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element minOccurs="0" name="Mounting">
                                <xs:annotation>
                                    <xs:documentation>Only in EDS: RFME-Montage.</xs:documentation>
                                    <xs:documentation>BOM-DB: Column AEH-RFME-M</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="1"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element minOccurs="0" name="SparePart">
                                <xs:annotation>
                                    <xs:documentation>Only in EDS: RFME-Ersatzteil.</xs:documentation>
                                    <xs:documentation>BOM-DB: Column AEH-RFME-E</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="1"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element minOccurs="0" name="Exchangeability">
                                <xs:annotation>
                                    <xs:documentation>Only in EDS: RFME-Austaschbarkeit.</xs:documentation>
                                    <xs:documentation>BOM-DB: Column AEH-RFME-A</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="1"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element minOccurs="0" name="Tool">
                                <xs:annotation>
                                    <xs:documentation>Only in EDS: RFME-Werkzeug.</xs:documentation>
                                    <xs:documentation>BOM-DB: Column AEH-RFME-W</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="1"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element minOccurs="0" name="Diagnose">
                                <xs:annotation>
                                    <xs:documentation>Only in EDS: RFME-Diagnose.</xs:documentation>
                                    <xs:documentation>BOM-DB: Column AEH-RFME-D</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="1"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element minOccurs="0" name="PieceNumberForecast">
                                <xs:annotation>
                                    <xs:documentation>Only in EDS: Prognosestückzahl.</xs:documentation>
                                    <xs:documentation>BOM-DB: Column AEH-RFME-PROGNOSE</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="6"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>

                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="PlantSupplyData" minOccurs="0">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element minOccurs="0" name="PlantSupplyExtractFlag">
                                <xs:annotation>
                                    <xs:documentation>Only in EDS: KZ zu Verteilerauswahl zum Teil.</xs:documentation>
                                    <xs:documentation>BOM-DB: Column AEH-KZVSTE</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="1"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element minOccurs="0" maxOccurs="30" ref="PlantSupplies">
                                <xs:annotation>
                                    <xs:documentation>Plant keys for production plants, to which the change notice is supplied.</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="ChangeNoticeTexts">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element minOccurs="0" maxOccurs="unbounded"
                                ref="ChangeNoticeLangData">
                                <xs:annotation>
                                    <xs:documentation>Language dependent data for german, english, spanish, portuguese, french, turkish, italian change notices.</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="K3">
        <xs:annotation>
            <xs:documentation>getT43RK3</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="Eco" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>ID of the ECO</xs:documentation>
                        <xs:documentation>BOM-DB: Column K3-KEM</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="Item" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Item, which is modified by the ECO</xs:documentation>
                        <xs:documentation>BOM-DB: Column K3-SNR</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="17"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="EcoVersion" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Version of the ECO</xs:documentation>
                        <xs:documentation>BOM-DB: Column K3-ASKE</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:integer">
                            <xs:minInclusive value="0"/>
                            <xs:maxInclusive value="999"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="TypeFlag" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Type of Item, which is modified by the ECO</xs:documentation>
                        <xs:documentation>BOM-DB: Column K3-TYPKZ</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="2"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="Position" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Position number of an Item inside the ECO-structure from ECO to Items</xs:documentation>
                        <xs:documentation>BOM-DB: Column K3-POS</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="3"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="ModificationFlag" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Indicates whether the change by ECO is done directly to an Item or only indirect by changing another Item directly and the viewed Item just indirectly.</xs:documentation>
                        <xs:documentation>BOM-DB: Column K3_ASKZ</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="1"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="EngineeringDate" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>BOM-DB: Column K3-S-DAT</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:dateTime"> </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="ManualFlag" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>BOM-DB: Column K3-MKZ</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="1"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <!-- itemContainer was not included, it is not relevant for ASPLM -->
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="EcoMasterData_LDI">
        <xs:annotation>
            <xs:documentation>getT43RKEMD</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="Eco" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>ECO to which master data in foreign language is searched</xs:documentation>
                        <xs:documentation>BOM-DB: Column KEMD-SNR</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" ref="EcoLangData">
                    <xs:annotation>
                        <xs:documentation>Language dependant data regarding EcoMasterData.</xs:documentation>
                        <xs:documentation>This structure contains information in all available languages except german.</xs:documentation>
                        <xs:documentation>The german data are stored in EcoMasterData.</xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="EcoLangData" type="Typ_EcoLanguageDependent">
        <xs:annotation>
            <xs:documentation>Language dependant data for the EcoMasterData in one language except german.</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="EcoLangDataMaster" type="Typ_EcoLanguageDependentMaster">
        <xs:annotation>
            <xs:documentation>Language dependant data for the EcoMasterdata for german language.</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="ChangeNoticeLangData" type="Typ_ChangeNoticeLanguageDependent"
        abstract="false">
        <xs:annotation>
            <xs:documentation>Language dependant data for the changeNotice in one language</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="PartsListLangData">
        <xs:complexType>
            <xs:sequence>
                <xs:group minOccurs="0" ref="Group_Versions"/>
                <xs:element minOccurs="0" ref="Description"/>
                <xs:element minOccurs="0" name="ExtendedDescription">
                    <xs:annotation>
                        <xs:documentation>Extended description of parts list in given language</xs:documentation>
                        <xs:documentation>BOM-DB: Column [suffix]_ERW_BEN</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="50"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element minOccurs="0" name="SteeringStatusDescription1"
                    type="Typ_SteeringStatusDescription"/>
                <xs:element minOccurs="0" name="SteeringStatusDescription2"
                    type="Typ_SteeringStatusDescription"/>
                <xs:element minOccurs="0" name="Remark">
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="352"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
            </xs:sequence>
            <xs:attribute ref="language" use="optional"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="S05PointOfUsageText">
        <xs:annotation>
            <xs:documentation>getT43RBKV: Point of usage text data of one or more given ModularPartsLists [=Assemblies (ZB) or PartsLists (SAA)]</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="PartsList" type="Typ_PartsList13" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>PartsList or Assembly</xs:documentation>
                        <xs:documentation>BOM-DB: BKV_SNR</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="Position">
                    <xs:annotation>
                        <xs:documentation>Position within Modular Parts List</xs:documentation>
                        <xs:documentation>BOM-DB: Column BKV_POS</xs:documentation>
                        <xs:documentation>Format in BOM-DB: Num(9), leading 000 stripped by EngBus service</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:pattern value="[0-9]+"/>
                            <xs:maxLength value="6"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:group minOccurs="0" ref="Group_ReleaseDates"/>
                <xs:group minOccurs="0" ref="Group_Eco"/>
                <xs:group minOccurs="0" ref="Group_Versions"/>
                <xs:element minOccurs="0" ref="StatusFrom">
                    <xs:annotation>
                        <xs:documentation>Status of version of S05, version generated by EcoFrom</xs:documentation>
                        <xs:documentation>BOM-DB: Column BKV_FRG_KZ_AB</xs:documentation>
                        <xs:documentation>Allocate Space to Value 0 (=S05 is released)
Allocate ‘NF’ to Value 1 (=S05 is not released)
</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" ref="StatusTo">
                    <xs:annotation>
                        <xs:documentation>Status of version of S05, version generated by EcoTo</xs:documentation>
                        <xs:documentation>BOM-DB: Column BKV_FRG_KZ_BIS</xs:documentation>
                        <xs:documentation>Allocate Space to Value 0 (=S05 is released)
Allocate ‘NF’ to Value 1 (=S05 is not released)</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="TextType">
                    <xs:annotation>
                        <xs:documentation>Specifies type of point of usage text </xs:documentation>
                        <xs:documentation>BOM-DB: Column BKV_BTXKZ</xs:documentation>
                        <xs:documentation>Possible values: V, T</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="1"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element minOccurs="0" name="PointOfUsageTextLangData">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="Text" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>Shows point of usage text</xs:documentation>
                                    <xs:documentation>BOM-DB: Column BKV_TEXT</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="300"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                        </xs:sequence>
                        <xs:attribute ref="language" use="optional"/>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="ModelMasterData">
        <xs:annotation>
            <xs:documentation>getT43RBM: Master data of one or more given Models (German: Baumuster). </xs:documentation>
            <xs:documentation>There are two types of Models, a Vehicle and a MajorComponent (German: Fahrzeug-Baumuster and Aggregate-Baumuster). The Service getT43RBM delivers master data to both types of Models.</xs:documentation>
            <xs:documentation>BOM-DB: Table T43RBM</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="ModelNumber" minOccurs="0"/>
                <xs:element minOccurs="0" ref="ModelLangData">
                    <xs:annotation>
                        <xs:documentation>German version of the language dependent Model data</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:group minOccurs="0" ref="Group_ReleaseDates"/>
                <xs:group minOccurs="0" ref="Group_EngineeringDates"/>
                <xs:group minOccurs="0" ref="Group_ManualFlags"/>
                <xs:group minOccurs="0" ref="Group_Eco"/>
                <xs:group minOccurs="0" ref="Group_Status"/>
                <xs:element minOccurs="0" ref="VersionToInvalidFlag"/>
                <xs:element minOccurs="0" name="ProductGroup">
                    <xs:annotation>
                        <xs:documentation>Product group of Model:
N (=Trucks), 
S (=Buses)</xs:documentation>
                        <xs:documentation>BOM-DB: Column BM_PGKZ</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="1"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element minOccurs="0" name="MajorComponent">
                    <xs:annotation>
                        <xs:documentation>Type of MajorComponent: 
H=1: Hinterachse,
J=2: Hinterachse, 
etc.
</xs:documentation>
                        <xs:documentation>BOM-DB: Column BM_AGRKZ</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="1"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:group minOccurs="0" ref="Group_DesignReleases"/>
                <xs:element minOccurs="0" ref="PlantSupplies"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="ModelMasterData_LDI">
        <xs:annotation>
            <xs:documentation>getT43RBMS: Model Master Data, Language Dependent Information</xs:documentation>
            <xs:documentation>Contains the master data of one or more given Models (German: Baumuster) in foreign languages</xs:documentation>
            <xs:documentation>There are two types of Models, a Vehicle and a MajorComponent (German: Fahrzeug-Baumuster and Aggregate-Baumuster). Service getT43RBMS delivers master data in foreign language to both types of Models.</xs:documentation>
            <xs:documentation>BOM-DB: Table T43RBMS</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="ModelNumber" minOccurs="0"/>
                <xs:element ref="ModelLangData" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Foreign language versions of the language dependent Model data</xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="ModuleMasterData">
        <xs:annotation>
            <xs:documentation>getT43RBMAG: Master data of one or more given Modules to one or more Models</xs:documentation>
            <xs:documentation>For simplicity, the Module of a Model, in German Baumustergruppe, might be named just Module.</xs:documentation>
            <xs:documentation>BOM-DB: Table T43RBMAG</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="ModelNumber" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Model (=Vehicle or MajorComponent)</xs:documentation>
                        <xs:documentation>BOM-DB: BMAG_SNR, (TODO: Check this:) digit 1 to 13</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element ref="ModuleNumber" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>BOM-DB: BMAG_SNR, digit 14 to 17</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" ref="ModuleLangData"/>
                <xs:group minOccurs="0" ref="Group_ReleaseDates"/>
                <xs:group minOccurs="0" ref="Group_EngineeringDates"/>
                <xs:group minOccurs="0" ref="Group_ManualFlags"/>
                <xs:group minOccurs="0" ref="Group_Eco"/>
                <xs:group minOccurs="0" ref="Group_Status"/>
                <xs:element minOccurs="0" ref="VersionToInvalidFlag"/>
                <xs:element minOccurs="0" name="DocumentationStatus">
                    <xs:annotation>
                        <xs:documentation>Values: UN, BL</xs:documentation>
                        <xs:documentation>BOM-DB: Column BMAG_STAT</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="2"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element minOccurs="0" name="ApprovedModelTypes">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element maxOccurs="8" name="ApprovedModelType">
                                <xs:annotation>
                                    <xs:documentation>For every ModelType, an approvalFlag describes the allowance of the ModelType within the Module of Model.</xs:documentation>
                                    <xs:documentation>Text content: ModelType defined to the Model in EDS/BCS</xs:documentation>
                                    <xs:documentation>BOM-DB: Column BMAG_AUSFA[i]  (i=1..8)</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:simpleContent>
                                        <xs:extension base="Typ_ModelType">
                                            <xs:attribute name="approval">
                                                <xs:annotation>
                                                  <xs:documentation>States whether the ModelType is activated to the Module</xs:documentation>
                                                  <xs:documentation>Value: S, B, Space</xs:documentation>
                                                  <xs:documentation>BOM-DB: Column BMAG_AUSFA_SPKZ[i]  (i=1..8)</xs:documentation>
                                                </xs:annotation>
                                                <xs:simpleType>
                                                  <xs:restriction base="xs:string">
                                                  <xs:maxLength value="1"/>
                                                  </xs:restriction>
                                                </xs:simpleType>
                                            </xs:attribute>
                                        </xs:extension>
                                    </xs:simpleContent>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" ref="LeadingDesignRelease"/>
                <xs:element minOccurs="0" ref="PlantSupplies"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="ModuleMasterData_LDI">
        <xs:annotation>
            <xs:documentation>getT43RBMAS: Master data of one or more given Modules (German: Baumuster-Gruppe) and/or one or more given ModelTypes (German: Baumuster-Ausführungsart) in foreign language</xs:documentation>
            <xs:documentation>There are two types of Modules, a VehicleModule and a MajorComponentModule (German: Fahrzeug-Baumuster-Gruppe and Aggregate-Baumuster-Gruppe). 
And there are two types of ModelTypes, a VehicleModelType and a MajorComponentModelType (German: Fahrzeug-Baumuster-Ausführungsart and Aggregate-Baumuster-Ausführungsart).
Service getT43RBMAS delivers master data in foreign language to all of these objects.</xs:documentation>
            <xs:documentation>BOM-DB: Table T43RBMAS</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="ModelNumber" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>BOM-DB: BMAS_SNR, (TODO: Check this:) digit 1 to 13</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element ref="ModuleNumber" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>BOM-DB: BMAS_SNR, digit 14 to 17</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" ref="ModuleLangData"/>
                <xs:element minOccurs="0" ref="ModelTypeLangData"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="ModelLangData">
        <xs:complexType>
            <xs:sequence>
                <xs:group minOccurs="0" ref="Group_Versions"/>
                <xs:element minOccurs="0" ref="Description"/>
                <xs:element minOccurs="0" name="SalesDescription">
                    <xs:annotation>
                        <xs:documentation>Description of Model for Sales (in the given language)</xs:documentation>
                        <xs:documentation>BOM-DB: Column BMS_VBEZ</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="30"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element minOccurs="0" name="Remark">
                    <xs:annotation>
                        <xs:documentation>Remark to the Model (in the given language)</xs:documentation>
                        <xs:documentation>BOM-DB: Column BMS_BEM</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="150"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element minOccurs="0" name="TechnicalData">
                    <xs:annotation>
                        <xs:documentation>Technical data to the Model (in the given language)</xs:documentation>
                        <xs:documentation>BOM-DB: Column BMS_TEDAT</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="200"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
            </xs:sequence>
            <xs:attribute ref="language" use="optional"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="ModuleLangData">
        <xs:complexType>
            <xs:sequence>
                <xs:group ref="Group_ModuleModelTypeDescriptions"/>
            </xs:sequence>
            <xs:attribute ref="language" use="optional"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="ModelTypeLangData">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="ModelTypeNumber"/>
                <xs:group ref="Group_ModuleModelTypeDescriptions"/>
            </xs:sequence>
            <xs:attribute ref="language" use="required"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="Item" type="Typ_PartNumber"> </xs:element>
    <xs:element name="ReleaseDateFrom" type="Typ_BomDbDate">
        <xs:annotation>
            <xs:documentation>Date/time from which on the parent element is valid</xs:documentation>
            <xs:documentation>BOM-DB: Column [suffix]_FRG_DAT_AB</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="ReleaseDateTo" type="Typ_BomDbDate">
        <xs:annotation>
            <xs:documentation>Date/time up to which the parent element is valid</xs:documentation>
